{
  "env": {
    "browser": true,
    "es2020": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "always",
        "json": "sometimes"
      }
    ]
  }
}
